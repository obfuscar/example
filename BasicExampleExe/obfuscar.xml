<?xml version="1.0" encoding="utf-8"?>
<Obfuscator>
  <Var name="InPath" value="bin\Release" />
  <Var name="OutPath" value="bin\Release\Obfuscated" />
  <Var name="KeepPublicApi" value="true" />
  <Var name="HidePrivateApi" value="true" />
  <Var name="RenameProperties" value="true" />
  <Var name="RenameEvents" value="true" />
  <Var name="RenameFields" value="true" />
  <Var name="RenameInternalTypes" value="true" />
  
  <Module path="$(InPath)\BasicExampleExe.exe" name="BasicExampleExe">
    <Type name="*" skipProperties="false" skipEvents="false" skipMethods="false" skipFields="false" skipEnums="false">
      <Attribute name="System.Reflection.ObfuscateAssemblyAttribute" exclude="true" />
      <Attribute name="System.Reflection.ObfuscationAttribute" exclude="true" />
    </Type>
  </Module>
  
  <Module path="$(InPath)\BasicExampleLibrary.dll" name="BasicExampleLibrary">
    <Type name="BasicExampleLibrary.*" skipPublic="true" />
  </Module>
</Obfuscator>
